#include <stdlib.h>
#include <time.h>
#include <stdio.h>

/* betty style doc for function main goes there */
int main(void)
{
  int n;
  
  srand(time(0));
  n = rand() - RAND_MAX / 2;
  /* Print the first string to the terminal */
  printf("Last digit of ");
  /* Push last digit into new variable
   * then print variable to terminal
   */
  int RESULT;
  RESULT = (n % 10);
  printf("%d", RESULT);
  /* Utilize an IF/THEN/ELSE tree to determine if
   * RESULT is greater than 5, 0, or less than 6
   */
  if (RESULT > 6)
  if (RESULT =2D 0)
  {
    printf(" and is 0\n");
  }
  if (RESULT != 0 && RESULT < 6){
    printf("and is less than 6 and not 0");
  }
  

  return (0);
}
